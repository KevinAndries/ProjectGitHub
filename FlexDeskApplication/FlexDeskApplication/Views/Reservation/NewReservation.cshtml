
@model MVC.ViewModels.ReservationViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div>
    <h4>@Model.Dictionary.Label28</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Model.Dictionary.Label2
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ReservationUser.StartDate)
        </dd>
        <dt>
            @Model.Dictionary.Label3
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ReservationUser.EndDate)
        </dd>
        <dt>
            @Model.Dictionary.Label26
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ReservationUser.NameCreator)
        </dd>
        <dt>
            @Model.Dictionary.Label27
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ReservationUser.CreationDate)
        </dd>
    </dl>

    @Html.ActionLink(@Model.Dictionary.Label18, "Index", "Reservation")
    
</div>

<br />

<div style="border:1px solid black">
    <p align="center">@Model.Building.Name.ToUpper(): @Model.ReservationFloor.Name.ToUpper()</p>
    <svg height="500" width="1000">
        @Html.Raw(Model.ReservationFloor.Svg.Replace("id=\"desk" + Model.ReservationUser.FlexDeskId + "\"", "id=\"" + Model.ReservationUser.FlexDeskId + "\" fill=\"green\" "))
    </svg>

</div>







