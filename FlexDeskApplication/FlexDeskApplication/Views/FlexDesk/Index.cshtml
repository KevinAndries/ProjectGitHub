@model MVC.ViewModels.FlexDeskViewModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.FlexDeskId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FlexDeskCode)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DepartmentId)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.FlexDesks) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FlexDeskId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FlexDeskCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepartmentId)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.FlexDeskId }) |
                @Html.ActionLink("Details", "Details", new { id = item.FlexDeskId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.FlexDeskId })
            </td>
        </tr>
}
    </tbody>
</table>

@Html.ActionLink("Back to Departments", "Index", "Department", new { floorId = Model.FloorId, buildingId = Model.BuildingId })

 
